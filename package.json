{
  "name": "@pbj/live.js",
  "version": "0.0.13",
  "main": "dist/sdk.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pbj-apps/live.js.git"
  },
  "author": "pbjlive <hello@pbj.live>",
  "license": "MIT",
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack",
    "build-sdk": "webpack --config webpack/webpack.sdk.config.js",
    "build-iframe": "webpack --config webpack/webpack.iframe.config.js",
    "start-iframe": "webpack NODE_ENV='development' --config webpack/webpack.iframe.config.js",
    "lint:staged": "lint-staged",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn run lint",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^4.5.1",
    "prettier": "2.1.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.2",
    "url-loader": "^4.1.1",
    "video.js": "^7.9.7",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@videojs/vhs-utils": "^2.2.1",
    "date-fns": "^2.27.0",
    "dotenv-webpack": "^4.0.0",
    "eslint": "^7.16.0",
    "html-loader": "^1.3.2",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "pre-commit": "^1.2.2",
    "query-string": "^7.1.1",
    "shopify-buy": "^2.14.0",
    "source-map-loader": "^2.0.0",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3"
  },
  "description": "PBJ Live Javascript SDK (Currently Docs only)",
  "bugs": {
    "url": "https://github.com/pbj-apps/live.js/issues"
  },
  "homepage": "https://github.com/pbj-apps/live.js#readme",
  "publishConfig": {
    "access": "public"
  }
}
